<connector>
  <version>5.0</version>
  <connectorId>GoogleCalendar - CreateEvenWithWebContent</connectorId>
  <categories>
    <category>
      <name>Google</name>
      <icon>org/bonitasoft/connectors/googlecalendar/google.png</icon>
    </category>
  </categories>
  <icon>google.png</icon>
  <inputs>
    <setter>
      <setterName>setUserEmail</setterName>
      <required></required>
      <parameters>
        <string>your_user@gmail.com</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPassword</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setCalendarUrl</setterName>
      <required></required>
      <parameters>
        <string>http://www.google.com/calendar/feeds/your_gmail_address/private/full</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setTitle</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setContent</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setStartTime</setterName>
      <parameters>
      	<string>2009-12-25T20:00:00+01:00</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setEndTime</setterName>
      <parameters>
      	<string>2009-12-25T23:00:00+01:00</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setIcon</setterName>
      <parameters>
        <string>http://www.google.com/favicon.ico</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setIconTitle</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setContentType</setterName>
      <required></required>
      <parameters>
        <string>text/html</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setUrl</setterName>
      <required></required>
      <parameters>
        <string>http://www.google.com</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setWidth</setterName>
      <required></required>
      <parameters>
        <string>400</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setHeight</setterName>
      <required></required>
      <parameters>
        <string>200</string>
      </parameters>
    </setter>
  </inputs>
  <outputs/>
  <pages>
    <page>
      <pageId>GoogleCalendarParameters</pageId>
      <widgets>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter"/>
          <labelId>userEmail</labelId>
        </text>
        <password>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[2]"/>
          <labelId>password</labelId>
        </password>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[3]"/>
          <labelId>calendarUrl</labelId>
        </text>
      </widgets>
    </page>
    <page>
      <pageId>FunctionParameters</pageId>
      <widgets>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[4]"/>
          <labelId>title</labelId>
        </text>
        <textarea>
          <rows>10</rows>
          <columns>60</columns>
          <maxChar>600</maxChar>
          <maxCharPerRow>60</maxCharPerRow>
          <setter reference="/connector/inputs/setter[5]"/>
          <labelId>content</labelId>
        </textarea>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[6]"/>
          <labelId>startTime</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[7]"/>
          <labelId>endTime</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[8]"/>
          <labelId>icon</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[9]"/>
          <labelId>iconTitle</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[10]"/>
          <labelId>contentType</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[11]"/>
          <labelId>url</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[12]"/>
          <labelId>width</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[13]"/>
          <labelId>height</labelId>
        </text>
      </widgets>
    </page>
  </pages>
</connector>