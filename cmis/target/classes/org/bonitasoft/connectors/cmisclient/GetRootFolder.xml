<connector>
  <connectorId>getRootFolder</connectorId>
  <version>5.0</version>
  <icon>cmis.png</icon>
  <categories>
    <category>
      <name>CMIS</name>
      <icon>org/bonitasoft/connectors/cmisclient/cmis.png</icon>
    </category>
  </categories>
  <inputs>
    <setter>
      <setterName>setUsername</setterName>
      <parameters>
        <string>admin</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPassword</setterName>
      <parameters>
        <string>admin</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setUrl</setterName>
      <required></required>
      <parameters>
        <string>http://cmis.alfresco.com/service/cmis</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setBinding_type</setterName>
      <required></required>
      <parameters>
        <string>ATOM</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setRepositoryName</setterName>
      <required></required>
      <parameters>
        <string>Main Repository</string>
      </parameters>
    </setter>
  </inputs>
  <outputs>
    <getter>
      <name>rootFolder</name>
      <metadata/>
    </getter>
  </outputs>
  <pages>
    <page>
      <pageId>ServerInvormations</pageId>
      <widgets>
        <text>
          <labelId>username</labelId>
          <setter reference="/connector/inputs/setter"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
        <password>
          <labelId>password</labelId>
          <setter reference="/connector/inputs/setter[2]"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </password>
        <text>
          <labelId>url</labelId>
          <setter reference="/connector/inputs/setter[3]"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
        <select>
          <labelId>binding_type</labelId>
          <setter reference="/connector/inputs/setter[4]"/>
          <values class="tree-map">
            <no-comparator/>
            <entry>
              <string>ATOM</string>
              <string>ATOM</string>
            </entry>
            <entry>
              <string>WebService</string>
              <string>WebService</string>
            </entry>
          </values>
          <editable>false</editable>
        </select>
      </widgets>
    </page>
    <page>
      <pageId>repositoryInformations</pageId>
      <widgets>
        <text>
          <labelId>repositoryId</labelId>
          <setter reference="/connector/inputs/setter[5]"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
      </widgets>
    </page>
  </pages>
</connector>