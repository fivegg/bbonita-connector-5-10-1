<connector>
  <connectorId>UpdateSObjectConnector</connectorId>
  <version>5.0</version>
  <icon>salesforce.png</icon>
  <categories>
    <category>
      <name>Salesforce</name>
      <icon>org/bonitasoft/connectors/salesforce/partner/salesforce.png</icon>
    </category>
  </categories>
  <inputs>
    <setter>
      <setterName>setUsername</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPassword_securityToken</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setAuthEndpoint</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setServiceEndpoint</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setRestEndpoint</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setProxyHost</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setProxyPort</setterName>
      <parameters>
        <int>0</int>
      </parameters>
    </setter>
    <setter>
      <setterName>setProxyUsername</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setProxyPassword</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setConnectionTimeout</setterName>
      <parameters>
        <int>0</int>
      </parameters>
    </setter>
    <setter>
      <setterName>setReadTimeout</setterName>
      <parameters>
        <int>0</int>
      </parameters>
    </setter>
    <setter>
      <setterName>setSObjectType</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setSObjectId</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setFieldValues</setterName>
      <required></required>
      <parameters>
        <map/>
      </parameters>
    </setter>
  </inputs>
  <outputs>
    <getter>
      <name>saveResult</name>
      <metadata/>
    </getter>
  </outputs>
  <pages>
    <page>
      <pageId>ConnectionInformation</pageId>
      <widgets>
        <text>
          <labelId>username</labelId>
          <setter reference="/connector/inputs/setter"/>
          <size>20</size>
          <maxChar>100</maxChar>
        </text>
        <password>
          <labelId>password_securityToken</labelId>
          <setter reference="/connector/inputs/setter[2]"/>
          <size>20</size>
          <maxChar>100</maxChar>
        </password>
        <group>
          <optional>true</optional>
          <labelId>endpoint</labelId>
          <widgets>
            <text>
              <labelId>authEndpoint</labelId>
              <setter reference="/connector/inputs/setter[3]"/>
              <size>20</size>
              <maxChar>100</maxChar>
            </text>
            <text>
              <labelId>serviceEndpoint</labelId>
              <setter reference="/connector/inputs/setter[4]"/>
              <size>20</size>
              <maxChar>100</maxChar>
            </text>
    	    <text>
              <labelId>restEndpoint</labelId>
              <setter reference="/connector/inputs/setter[5]"/>
              <size>20</size>
              <maxChar>100</maxChar>
            </text>
          </widgets>
        </group>
        <group>
          <optional>true</optional>
          <labelId>proxy</labelId>
          <widgets>
            <text>
              <labelId>proxyHost</labelId>
              <setter reference="/connector/inputs/setter[6]"/>
              <size>20</size>
              <maxChar>100</maxChar>
            </text>
            <text>
              <labelId>proxyPort</labelId>
              <setter reference="/connector/inputs/setter[7]"/>
              <size>20</size>
              <maxChar>5</maxChar>
            </text>
            <text>
              <labelId>proxyUsername</labelId>
              <setter reference="/connector/inputs/setter[8]"/>
              <size>20</size>
              <maxChar>100</maxChar>
            </text>
            <password>
              <labelId>proxyPassword</labelId>
              <setter reference="/connector/inputs/setter[9]"/>
              <size>20</size>
              <maxChar>100</maxChar>
            </password>
          </widgets>
        </group>
        <group>
          <optional>true</optional>
          <labelId>timeouts</labelId>
          <widgets>
            <text>
              <labelId>connectionTimeout</labelId>
              <setter reference="/connector/inputs/setter[10]"/>
              <size>20</size>
              <maxChar>100</maxChar>
            </text>
            <text>
              <labelId>readTimeout</labelId>
              <setter reference="/connector/inputs/setter[11]"/>
              <size>20</size>
              <maxChar>100</maxChar>
            </text>
          </widgets>
        </group>
      </widgets>
    </page>
    <page>
      <pageId>Parameters</pageId>
      <widgets>
        <text>
          <labelId>sObjectType</labelId>
          <setter reference="/connector/inputs/setter[12]"/>
          <size>20</size>
          <maxChar>100</maxChar>
        </text>
        <text>
          <labelId>sObjectId</labelId>
          <setter reference="/connector/inputs/setter[13]"/>
          <size>20</size>
          <maxChar>100</maxChar>
        </text>
        <array>
          <labelId>fieldValues</labelId>
          <setter reference="/connector/inputs/setter[14]"/>
          <cols>2</cols>
          <rows>1</rows>
          <fixedCols>true</fixedCols>
          <fixedRows>false</fixedRows>
          <colsCaptions>
            <string>Field Name</string>
            <string>Field Value</string>
          </colsCaptions>
        </array>
      </widgets>
    </page>
  </pages>
</connector>