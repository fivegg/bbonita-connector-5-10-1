<connector>
  <version>5.0</version>
  <connectorId>Exo - VersionControl</connectorId>
  <categories>
    <category>
      <name>eXo</name>
      <icon>org/bonitasoft/connectors/webdav/exo/eXoDMS.gif</icon>
    </category>
  </categories>
  <icon>eXoDMS.gif</icon>
  <inputs>
    <setter>
      <setterName>setHost</setterName>
      <required></required>
      <parameters>
        <string>localhost</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPort</setterName>
      <required></required>
      <parameters>
        <long>8080</long>
      </parameters>
    </setter>
    <setter>
      <setterName>setUsername</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPassword</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setUri</setterName>
      <required></required>
      <parameters>
        <string>http://localhost:8080/rest/private/jcr/repository/collaboration</string>
      </parameters>
    </setter>
  </inputs>
  <outputs>
    <getter>
      <name>statusCode</name>
    </getter>
    <getter>
      <name>statusText</name>
    </getter>
    <getter>
      <name>response</name>
    </getter>
  </outputs>
  <pages>
    <page>
      <pageId>eXoConfiguration</pageId>
      <widgets>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter"/>
          <labelId>host</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[2]"/>
          <labelId>port</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[3]"/>
          <labelId>username</labelId>
        </text>
        <password>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[4]"/>
          <labelId>password</labelId>
        </password>
      </widgets>
    </page>
    <page>
      <pageId>FunctionParameters</pageId>
      <widgets>
        <text>
          <size>20</size>
          <maxChar>60</maxChar>
          <setter reference="/connector/inputs/setter[5]"/>
          <labelId>uri</labelId>
        </text>
      </widgets>
    </page>
  </pages>
</connector>