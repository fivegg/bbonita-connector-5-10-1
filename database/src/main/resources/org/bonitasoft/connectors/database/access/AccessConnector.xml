<connector>
  <version>5.0</version>
  <connectorId>Access</connectorId>
  <icon>access.png</icon>
  <categories>
    <category>
      <name>Database</name>
      <icon>org/bonitasoft/connectors/database/database.png</icon>
    </category>
  </categories>
  <inputs>
    <setter>
      <setterName>setDatabase</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setQuery</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setUsername</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPassword</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
  </inputs>
  <outputs>
    <getter>
      <name>rowSet</name>
    </getter>
  </outputs>
  <pages>
    <page>
      <pageId>db</pageId>
      <widgets>
        <text>
          <size>30</size>
          <maxChar>50</maxChar>
          <setter reference="/connector/inputs/setter"/>
          <labelId>database</labelId>
        </text>
        <text>
          <size>30</size>
          <maxChar>50</maxChar>
          <setter reference="/connector/inputs/setter[3]"/>
          <labelId>username</labelId>
        </text>
        <password>
          <size>30</size>
          <maxChar>50</maxChar>
          <setter reference="/connector/inputs/setter[4]"/>
          <labelId>password</labelId>
        </password>
      </widgets>
    </page>
    <page>
      <pageId>qry</pageId>
      <widgets>
        <textarea>
          <rows>10</rows>
          <columns>60</columns>
          <maxChar>600</maxChar>
          <maxCharPerRow>60</maxCharPerRow>
          <setter reference="/connector/inputs/setter[2]"/>
          <labelId>query</labelId>
        </textarea>
      </widgets>
    </page>
  </pages>
</connector>