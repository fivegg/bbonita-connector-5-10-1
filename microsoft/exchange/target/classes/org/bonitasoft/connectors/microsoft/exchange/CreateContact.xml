<connector>
  <connectorId>CreateContact</connectorId>
  <version>5.0</version>
  <icon>exchange.png</icon>
  <categories>
    <category>
      <name>Exchange</name>
      <icon>org/bonitasoft/connectors/microsoft/exchange/exchange.png</icon>
    </category>
  </categories>
  <inputs>
    <setter>
      <setterName>setUsername</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPassword</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setServerLocation</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>    
    <setter>
      <setterName>setGivenName</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setSurname</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setCompanyName</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setJobTitle</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPhonenumber</setterName>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setEmail</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
  </inputs>
  <outputs>
    <getter>
      <name>statusCode</name>
    </getter>
    <getter>
      <name>statusText</name>
    </getter>
    <getter>
      <name>response</name>
    </getter>
  </outputs>
  <pages>
    <page>
      <pageId>ExchangesServerParameters</pageId>
      <widgets>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter"/>
          <labelId>username</labelId>
        </text>
        <password>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[2]"/>
          <labelId>password</labelId>
        </password>
        <select>
          <values>
            <entry>
              <string>Europe, Middle East and Africa</string>
              <string>emea</string>
            </entry>
            <entry>
              <string>North America</string>
              <string>northamerica</string>
            </entry>
            <entry>
              <string>Asia-Pacific</string>
              <string>apac</string>
            </entry>
          </values>
          <editable>false</editable>
          <top>
              <string>Europe, Middle East and Africa</string>
              <string>emea</string>
          </top>
          <setter reference="/connector/inputs/setter[3]"/>
          <labelId>serverLocation</labelId>
        </select>        
      </widgets>
    </page>
    <page>
      <pageId>NewContactParameters</pageId>
      <widgets>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[4]"/>
          <labelId>givenName</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[5]"/>
          <labelId>surname</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[6]"/>
          <labelId>companyName</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[7]"/>
          <labelId>jobTitle</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[8]"/>
          <labelId>phonenumber</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[9]"/>
          <labelId>email</labelId>
        </text>
      </widgets>
    </page>
  </pages>
</connector>