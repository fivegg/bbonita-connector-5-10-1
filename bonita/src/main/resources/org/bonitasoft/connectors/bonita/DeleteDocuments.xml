<connector>
	<connectorId>DeleteDocuments</connectorId>
	<version>5.0</version>
	<icon>bonita.png</icon>
	<categories>
		<category>
			<name>Bonita</name>
			<icon>org/bonitasoft/connectors/bonita.png</icon>
		</category>
	</categories>
	<inputs>
		<setter>
			<setterName>setParameters</setterName>
			<required />
			<parameters>
				<map />
			</parameters>
		</setter>
	</inputs>
	<outputs>
		<getter>
			<name>success</name>
			<metadata />
		</getter>
	</outputs>
	<pages>
		<page>
			<pageId>DocumentInformation</pageId>
			<widgets>
				<array>
					<labelId>parameters</labelId>
					<setter reference="/connector/inputs/setter" />
					<cols>2</cols>
					<rows>1</rows>
					<fixedCols>true</fixedCols>
					<fixedRows>false</fixedRows>
					<colsCaptions>
						<string>documentUUID</string>
						<string>allVersions</string>
					</colsCaptions>
				</array>
			</widgets>
		</page>
	</pages>
</connector>