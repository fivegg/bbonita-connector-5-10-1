<connector>
  <version>5.0</version>
  <connectorId>TalendJobLauncher</connectorId>
  <icon>talend.png</icon>
  <categories>
    <category>
      <name>Talend</name>
      <icon>org/bonitasoft/connectors/talend/talend.png</icon>
    </category>
  </categories>
  <inputs>
    <setter>
      <setterName>setProjectName</setterName>
      <required/>
      <parameters>
        <string/>
      </parameters>
    </setter>
    <setter>
      <setterName>setJobName</setterName>
      <required></required>
      <parameters>
        <string/>
      </parameters>
    </setter>
    <setter>
      <setterName>setJobVersion</setterName>
      <required/>
      <parameters>
        <string>0.1</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setJobParameters</setterName>
      <parameters>
        <map/>
      </parameters>
    </setter>
    <setter>
      <setterName>setPrintOutput</setterName>
      <parameters>
        <boolean>true</boolean>
      </parameters>
    </setter>
  </inputs>
  <outputs>
    <getter>
      <name>bufferOutput</name>
    </getter>
  </outputs>
  <pages>
    <page>
      <pageId>job</pageId>
      <widgets>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter"/>
          <labelId>projectName</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[2]"/>
          <labelId>jobName</labelId>
        </text>
        <text>
          <size>5</size>
          <maxChar>10</maxChar>
          <setter reference="/connector/inputs/setter[3]"/>
          <labelId>jobVersion</labelId>
        </text>
        <array>
          <labelId>jobParameters</labelId>
          <cols>2</cols>
          <rows>0</rows>
          <fixedCols>true</fixedCols>
          <fixedRows>false</fixedRows>
          <colsCaptions>
            <string>name</string>
            <string>value</string>
          </colsCaptions>
          <setter reference="/connector/inputs/setter[4]"/>
        </array>
        <checkbox>
          <name>printOutput</name>
          <value>true</value>
          <setter reference="/connector/inputs/setter[5]"/>
          <labelId>printOutput</labelId>
        </checkbox>
      </widgets>
    </page>
  </pages>
</connector>