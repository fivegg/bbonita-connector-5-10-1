<connector>
  <version>5.0</version>
  <connectorId>AlfrescoCheckin</connectorId>
  <categories>
    <category>
      <name>Alfresco</name>
      <icon>org/bonitasoft/connectors/alfresco/alfresco.jpg</icon>
    </category>
  </categories>
  <icon>alfresco.jpg</icon>
  <inputs>
    <setter>
      <setterName>setHost</setterName>
      <required></required>
      <parameters>
        <string>localhost</string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPort</setterName>
      <required></required>
      <parameters>
        <long>8080</long>
      </parameters>
    </setter>
    <setter>
      <setterName>setUsername</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setPassword</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setCheckedOutFileId</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setIsMajorVersion</setterName>
      <required></required>
      <parameters>
        <boolean>true</boolean>
      </parameters>
    </setter>
    <setter>
      <setterName>setCheckinComments</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
  </inputs>
  <outputs>
    <getter>
      <name>responseDocument</name>
    </getter>
    <getter>
      <name>responseType</name>
    </getter>
    <getter>
      <name>statusCode</name>
    </getter>
    <getter>
      <name>statusText</name>
    </getter>
    <getter>
      <name>stackTrace</name>
    </getter>
  </outputs>
  <pages>
    <page>
      <pageId>AlfrescoConfiguration</pageId>
      <widgets>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter"/>
          <labelId>host</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[2]"/>
          <labelId>port</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[3]"/>
          <labelId>username</labelId>
        </text>
        <password>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[4]"/>
          <labelId>password</labelId>
        </password>
      </widgets>
    </page>
    <page>
      <pageId>FunctionParameters</pageId>
      <widgets>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[5]"/>
          <labelId>checkedOutFileId</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[6]"/>
          <labelId>isMajorVersion</labelId>
        </text>
        <text>
          <size>20</size>
          <maxChar>40</maxChar>
          <setter reference="/connector/inputs/setter[7]"/>
          <labelId>checkinComments</labelId>
        </text>
      </widgets>
    </page>
  </pages>
</connector>